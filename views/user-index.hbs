{{#if manageGroup}}
<div class="panel panel-default">
  <div class="panel-body">
      <a href="/group/{{manageGroup}}">{{manageGroup}}</a>
  </div>
</div>
{{/if}}

<div class="panel panel-default">
  <div class="panel-body">
    <ul class="list-group">
      {{#each allUsers}}
        <li class="list-group-item">
          <a href="/user/{{this._id}}">{{this.firstName}} {{this.lastName}}</a>
        </li>
      {{/each}}
    </ul>
  </div>
</div>
